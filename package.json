  "bugs": {
    "url": "https://github.com/ebabel-games/dec-2018/issues"
  },
  "homepage": "https://github.com/ebabel-games/dec-2018#readme",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "har-validator": "^5.1.3",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "raw-loader": "^0.5.1",
    "webpack": "^3.12.0",
